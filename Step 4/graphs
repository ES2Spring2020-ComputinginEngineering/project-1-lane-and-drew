import numpy as np
import matplotlib.pyplot as plt

lengths = np.array([.71,.61,.51,.41,.31])

def period(a):
    periods = [0]*len(a)
    for i in range(len(a)):
        p = 2*np.pi*np.sqrt(a[i]/9.81)
        periods[i] = p
    return periods
print(period(lengths))

